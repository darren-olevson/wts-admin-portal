# Harbor API Configuration
# Client/Account API (for clients, accounts, documents)
HARBOR_BASE_URL=https://bo-api.development.wedbush.tech/client-account/v1

# Payments API (for payment instructions/withdrawals)
HARBOR_PAYMENTS_BASE_URL=https://bo-api.production.wedbush.tech/v1

# Authentication (flexible - supports API key, Bearer token, or both)
HARBOR_API_KEY=your-api-key-here
HARBOR_BEARER_TOKEN=your-bearer-token-here

# Environment (development, staging, production)
HARBOR_ENVIRONMENT=development

# Stytch Authentication
STYTCH_PROJECT_ID=your-stytch-project-id
STYTCH_SECRET=your-stytch-secret

# AWS S3 Configuration
AWS_S3_BUCKET=wts-documents
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key

# Server Configuration
PORT=3001
NODE_ENV=development
